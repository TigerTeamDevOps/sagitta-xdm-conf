#!/bin/sh
#
# This script is run as root before showing the x11 splash screen.
# Automatically generated by Sagitta HPC.

export PATH=/bin:/usr/bin
export HOME=/root

xset -dpms
xset s off
xhost +

od6config --set core=1000,mem=1200,power=10,temp=95,fan=75

feh --bg-scale /usr/share/images/xdm-background.png

